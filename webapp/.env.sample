# Sample .env file for AnonyConnect Webapp
# Copy this file to .env and fill in your values

# Node environment
NODE_ENV=production

# Admin username for login
ADMIN_USERNAME=admin

# Admin password (set a strong password in production)
ADMIN_PASSWORD=changeme

# Session secret for cookies (use a long random string)
SESSION_SECRET=your_session_secret_here

# Port for Next.js (default: 3000)
PORT=3000

# Example: Telegram bot token (if used)
# TELEGRAM_BOT_TOKEN=your_telegram_bot_token

# Cloudflare Turnstile site key (if used)
CF_TURNSTILE_SITE_KEY=your_site_key
CF_TURNSTILE_SECRET_KEY=your_secret_key
